#include <stdio.h>

void decToBinary(int number)
{
    int i = 0;
    int binary = 0;
    while(number>0)
    {
         binary = number%2;
        number = number/2;
        printf("%d\n", binary);
    }
}

int main()
{
    int number;
    printf("Numero em decimal:");
    scanf("%d", &number);
    decToBinary(number);
    return 0;
}
